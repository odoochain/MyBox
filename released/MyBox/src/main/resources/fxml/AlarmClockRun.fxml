<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="thisPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="740.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.AlarmClockRunController">
    <children>
        <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" prefWidth="512.0" spacing="10.0">
            <children>
                <Label text="%AlarmClock" HBox.hgrow="NEVER">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Label fx:id="descLabel" maxWidth="1.7976931348623157E308" prefWidth="466.0" text="Desc" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
            <children>
                <Label fx:id="timeLabel" text="time" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
            <children>
                <Label fx:id="soundLabel" maxWidth="1.7976931348623157E308" text="Sound" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <HBox alignment="CENTER_RIGHT" spacing="10.0">
            <children>
                <Button mnemonicParsing="false" onAction="#manageAction" text="%ManageAlarms" />
                <Button layoutX="432.0" layoutY="10.0" mnemonicParsing="false" onAction="#inactive" text="%Inactive" />
                <Button mnemonicParsing="false" onAction="#knowAction" text="%GotIt" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
