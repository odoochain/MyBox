<?import javafx.scene.image.*?>
<?import javafx.scene.media.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.AboutController">
   <children>
      <MediaView fitHeight="100.0" fitWidth="500.0" />
      <HBox fx:id="mediaBar" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="playButton" disable="true" mnemonicParsing="false" onAction="#playAction">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconPlay.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label text="%Time" />
            <Slider fx:id="timeSlider" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <Label fx:id="timeLabel" />
            <Label text="%VoiceVolumn" />
            <Slider fx:id="volumeSlider" prefWidth="70.0" />
         </children>
      </HBox>
   </children>
</VBox>
