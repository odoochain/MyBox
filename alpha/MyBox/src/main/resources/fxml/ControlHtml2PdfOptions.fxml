<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ControlHtml2PdfOptions">
   <children>
      <fx:include fx:id="ttf" source="ControlTTFSelector.fxml" />
      <Label text="%HtmlToPdfStyle" />
      <TextArea fx:id="cssArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <CheckBox fx:id="ignoreHeadCheck" mnemonicParsing="false" text="%IgnoreHtmlHead" />
   </children>
</VBox>
